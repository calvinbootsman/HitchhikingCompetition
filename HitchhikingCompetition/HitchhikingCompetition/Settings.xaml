<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HitchhikingCompetition.Settings">
    <ContentPage.Content>
        <StackLayout VerticalOptions="FillAndExpand"
                     HorizontalOptions="FillAndExpand"
                     Orientation="Vertical"
                     Spacing="15">
            <Button Text="Remove Log in" x:Name="Test" Clicked="TestIsClicked"/>
            <Button Text="Remove crazy88" x:Name="RemoveCrazy" Clicked="RemoveList"/>
            <Button Text="Get Location" x:Name="LocationBtn" Clicked="GetLocation"/>
            <Label x:Name="Longitudelbl" Text=""/>
            <Label x:Name="Latitudelbl" Text=""/>
            <Label x:Name="Speedlbl" Text=""/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>